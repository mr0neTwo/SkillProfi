<Window x:Class="SkillProfi.WfpClient.Modules.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkillProfi.WfpClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Login" Width="100" Height="30" Click="Login_Click"/>
            <Button Content="LoadMessages" Width="100" Height="30" Click="LoadMessages_Click" Margin="0,10,0,0"/>
        </StackPanel>
        
    </Grid>
</Window>
